package com.nttdata;
import org.slf4j.Logger;
import org.slf4j.LoggerFactory;
import java.util.*;
/**
 * Hello world!
 *
 */
public class App 
{
	/** Logger */
	private static final Logger CHIVATO = LoggerFactory.getLogger(App.class);
	
	/**
	 * Metodo principal
	 * @param args
	 */
    public static void main( String[] args )
    {
    	List lista = new ArrayList();   
    	CHIVATO.info("Creación de una lista");
    	
    	lista.add("Juan");
        lista.add("Ana");
        lista.add("Pedro");
        lista.add("Miguel");
        CHIVATO.info("Nombres añadidos");
        
        
        
        
        
    }
}
